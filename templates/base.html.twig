<!DOCTYPE html>
<html lang="fr">
<head>
    {% block head %}
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

   

    <!-- Fontfaces CSS-->
    <link href="css/font-face.css" rel="stylesheet" media="all">
    <link href="dist/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="dist/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="dist/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="dist/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- dist CSS-->
    <link href="dist/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="dist/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="dist/wow/animate.css" rel="stylesheet" media="all">
    <link href="dist/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="dist/slick/slick.css" rel="stylesheet" media="all">
    <link href="dist/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="dist/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="css/theme.css" rel="stylesheet" media="all">
   
    <title>{% block title %}
       Base Twig
    {% endblock title %} </title>
    {% endblock head %}
</head>
<body class="animsition">
    <main>
        {% block content %}
        {% endblock content %}

    </main>
    
 
<footer>
    {% block javascript %}
      <!-- Jquery JS-->
      <script src="dist/jquery-3.2.1.min.js"></script>
      <!-- Bootstrap JS-->
      <script src="dist/bootstrap-4.1/popper.min.js"></script>
      <script src="dist/bootstrap-4.1/bootstrap.min.js"></script>
      <!-- dist JS       -->
      <script src="dist/slick/slick.min.js">
      </script>
      <script src="dist/wow/wow.min.js"></script>
      <script src="dist/animsition/animsition.min.js"></script>
      <script src="dist/bootstrap-progressbar/bootstrap-progressbar.min.js">
      </script>
      <script src="dist/counter-up/jquery.waypoints.min.js"></script>
      <script src="dist/counter-up/jquery.counterup.min.js">
      </script>
      <script src="dist/circle-progress/circle-progress.min.js"></script>
      <script src="dist/perfect-scrollbar/perfect-scrollbar.js"></script>
      <script src="dist/chartjs/Chart.bundle.min.js"></script>
      <script src="dist/select2/select2.min.js">
      </script>
  
      <!-- Main JS-->
      <script src="js/main.js"></script>
    {% endblock javascript %}

</footer>
   
</body>
</html>